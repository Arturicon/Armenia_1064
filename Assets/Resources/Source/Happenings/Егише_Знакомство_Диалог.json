          "$type": "DialogPhrase",
          "IsCameraShow": true,
          "IsShowFace": false,
          "PersonName": {
            "$type": "PersonName",
            "Name": "Барсег"
          },
          "Index": 4,
          "Text": "В жизни не слышал такого вздора."
