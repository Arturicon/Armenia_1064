            "IsQuestCons": false,
            "IsPersonCons": false,
            "IsHappenCons": false,
            "IsMessage": false
          },
          "AfterAction": {
            "$type": "AfterAction",
            "Immediately": true,
            "SetupCamp": false,
            "DialogAvailable": 1,
            "LeaveCamp": false,
            "IsAfterAction": false,
            "IsActivateQuest": false
          }
        }
      ],
      "Advices": [],
      "IsFocus": false,
      "IsShowFace": false
