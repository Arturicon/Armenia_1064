        {
          "$type": "DialogPhrase",
          "IsCameraShow": true,
          "IsShowFace": false,
          "PersonName": {
            "$type": "PersonName",
            "Name": "Барсег"
          },
          "Index": 3,
          "Text": "Лучше пропусти нас и каждый пойдет своей дорогой, без лишней крови."
        },
        {
          "$type": "DialogPhrase",
          "IsCameraShow": false,
